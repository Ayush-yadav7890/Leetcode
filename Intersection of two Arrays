class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        boolean[] seen = new boolean[1001];

        for (int num : nums1) {
            seen[num] = true;
        }

        boolean[] add = new boolean[1001];

        Set<Integer> resSet = new HashSet<>();
        for (int num : nums2) {
            if (seen[num] == true) {
                resSet.add(num);
            }
        }

        int[] res = new int[resSet.size()];
        int i = 0;

        for (int num : resSet) {
            res[i++] = num;
        }
        return res;
    }
}
